
@{
    ViewBag.Title = "Delete";
    var item = ViewBag.empobj as HTMlCrud.EDM.tblemployee;
}

<h2>Delete</h2>

<div class="container">
    <dl class="dl-horizontal">
        <dt>Employee ID</dt>
        <dd>@item.emp_id</dd>

        <dt>First Name</dt>
        <dd>@item.f_name</dd>

        <dt>salary</dt>
        <dd>@item.salary</dd>

        <dt>Email</dt>
        <dd>@item.email</dd>

        <dt>Mobile</dt>
        <dd>@item.mobile</dd>

        <dt>Gender</dt>
        <dd>@item.gender</dd>

    </dl>

    @using (Html.BeginForm())
    {
        <input type="submit" value="Delete" class="btn btn-danger" />
    }
</div>